{
  "chmod": "0755",
  "directories": [
    "src"
  ],
  "files": [
    "LICENSE",
    "vendor/autoload.php"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "tests",
        "Tests",
        "examples"
      ],
      "in": [
        "vendor/composer",
        "vendor/doctrine",
        "vendor/hamcrest",
        "vendor/monolog",
        "vendor/myclabs",
        "vendor/phar-io",
        "vendor/psr",
        "vendor/symfony",
        "vendor/theseer"
      ]
    }
  ],
  "git-version": "package_version",
  "main": "bin/m4b-tool.php",
  "output": "dist/m4b-tool.phar",
  "stub": true
}